<h1>Listing users</h1>

<table>
  <tr>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to t('link.show'), user %></td>
    <td><%= link_to t('link.edit'), edit_user_path(user) %></td>
    <td><%= link_to t('link.destroy'), user, :confirm => t('notice.are_you_sure'), :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>